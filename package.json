{
	"name": "spor-instantbul",
	"displayName": "SPOR INSTANTBUL",
	"version": "0.8.2",
	"description": "Helps with getting quick reservations through 'https://online.spor.istanbul/'",
	"author": "Alper Akta≈ü <contact@aktasalper.com>",
	"homepage": "https://github.com/aktasalper/spor-instantbul",
	"scripts": {
		"dev": "plasmo dev",
		"build": "plasmo build --source-maps",
		"build-firefox": "pnpm build --target=firefox-mv2",
		"test": "plasmo test"
	},
	"dependencies": {
		"@plasmohq/messaging": "^0.6.1",
		"@plasmohq/storage": "^1.9.0",
		"plasmo": "0.84.1",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@types/chrome": "0.0.251",
		"@types/node": "20.9.0",
		"@types/react": "18.2.37",
		"@types/react-dom": "18.2.15",
		"autoprefixer": "^10.4.17",
		"postcss": "^8.4.33",
		"prettier": "3.0.3",
		"tailwindcss": "^3.4.1",
		"type-fest": "^4.10.1",
		"typescript": "5.2.2"
	},
	"manifest": {
		"host_permissions": [
			"*://online.spor.istanbul/*"
		],
		"permissions": [
			"activeTab",
			"storage"
		],
		"browser_specific_settings": {
			"gecko": {
				"id": "spor-instantbul@aktasalper.com"
			}
		}
	}
}
